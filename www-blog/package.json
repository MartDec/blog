    "@typescript-eslint/parser": "^6.14.0",
    "@vue/eslint-config-airbnb-with-typescript": "^7.0.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
